import { store } from '../../redux/store.js';
import counterSelector from '../../redux/selector/counterSelector.js'


$ {
  const count = counterSelector(store.getState())
}

<div.counter>
  <p>${count}</p>
  <app-button onClick("increment")> + </app-button>
  <app-button onclick("decrement")> - </app-button>
  <app-button onClick("incrementByFive")>
    Add 5
  </app-button>
</div>
